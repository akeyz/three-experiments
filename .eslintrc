{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": "off",
    "no-mixed-operators": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off"
  },
  "globals": {
    "THREE": true,
    "THREEx": true,
    "window": true,
    "document": true,
    "requestAnimationFrame": true
  }
}
